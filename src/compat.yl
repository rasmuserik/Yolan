[; make sure that the environment is sane]

[if [[console jsType] fails] [set console [#]]]

[if [[console 'log] fails] [console set 'log [fn [] [return true]]]]

[if [[console 'error] fails] [console set 'error [console 'log]]]

[if [[console 'info] fails] [console set 'info [console 'log]]]

[if [[console 'warn] fails] [console set 'warn [console 'log]]]
