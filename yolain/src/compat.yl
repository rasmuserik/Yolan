[# make sure that the environment is sane]

[if-else [[console jsType] fails] [set console [new object]]]

[if-else [[console get 'log] fails] [console set 'log [fn [] true]]]

[if-else [[console get 'error] fails]
  [console set 'error [console get 'log]]]

[if-else [[console get 'info] fails] [console set 'info [console get 'log]]]

[if-else [[console get 'warn] fails] [console set 'warn [console get 'log]]]